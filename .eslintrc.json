{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "quotes": ["error", "single", {"allowTemplateLiterals": true} ],
        "linebreak-style": 0,
        "no-use-before-define": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "import/newline-after-import": 0,
        "max-len": 0
    }
}
