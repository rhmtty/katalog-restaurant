{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "quotes": ["error", "single"],
        "linebreak-style": 0,
        "no-use-before-define": 0,
        "import/no-extraneous-dependencies": 0
    }
}
